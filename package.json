{
  "name": "@bouzuya/expand-markdown-anchors",
  "description": "expand-markdown-anchors",
  "version": "0.2.0",
  "author": {
    "name": "bouzuya",
    "email": "m@bouzuya.net",
    "url": "http://bouzuya.net"
  },
  "babel": {
    "presets": [
      "es2017",
      "power-assert"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/expand-markdown-anchors/issues"
  },
  "devDependencies": {
    "@types/node": "^8.0.13",
    "@types/power-assert": "^1.4.29",
    "@types/proxyquire": "1.3.27",
    "@types/sinon": "^2.3.2",
    "babel-cli": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-power-assert": "^1.0.0",
    "beater": "5.0.0",
    "cpx": "1.5.0",
    "npm-run-all": "^4.0.2",
    "power-assert": "^1.4.4",
    "proxyquire": "^1.8.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.8",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1",
    "watch": "^1.0.2"
  },
  "homepage": "https://github.com/bouzuya/expand-markdown-anchors#readme",
  "keywords": [],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bouzuya/expand-markdown-anchors"
  },
  "scripts": {
    "build": "npm-run-all -s 'build:ts' 'build:es2017' -p 'copy:*'",
    "build:es2017": "babel --out-dir .tmp/es5/ .tmp/es2017/",
    "build:ts": "tsc",
    "clean": "rimraf .tmp lib",
    "copy:dts": "cpx '.tmp/es2017/src/**/*.d.ts' 'lib/'",
    "copy:js": "cpx '.tmp/es5/src/**/*.js' 'lib/'",
    "lint": "npm-run-all -p 'lint:*'",
    "lint:ts": "tslint --project tsconfig.json",
    "prepublish": "npm-run-all -s 'clean' -p 'lint' 'build'",
    "start": "node examples/index.js",
    "test": "node .tmp/es5/test/index.js",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:es2017": "npm run 'build:es2017' -- --watch",
    "watch:lint": "watch --wait 2 'npm run lint' 'src/' 'test/'",
    "watch:test": "watch --wait 2 'npm test' '.tmp/es5/'",
    "watch:ts": "npm run 'build:ts' -- --watch"
  },
  "typings": "lib/index.d.ts"
}
